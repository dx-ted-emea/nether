<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp1.1;net46</TargetFrameworks>
    <Version>0.0.2-alpha</Version>
    <Authors>MicrosoftDX</Authors>
    <Company>MicrosoftDX</Company>
    <PackageLicenseUrl>https://raw.githubusercontent.com/MicrosoftDX/nether/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/microsoftdx/nether</PackageProjectUrl>
    <PackageIconUrl>https://github.com/MicrosoftDX/nether/raw/master/logos/logo-only/nether-logo-256x256.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/MicrosoftDX/nether.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>gaming, azure, nether, unity</PackageTags>
    <Description>Nether, a library to help you build gaming backends on Microsoft Azure.</Description>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="1.1.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="1.1.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Nether.Analytics\Nether.Analytics.csproj" />
  </ItemGroup>

</Project>